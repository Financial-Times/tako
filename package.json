{
  "name": "tako",
  "version": "1.0.0",
  "description": "A GitHub application that helps maintain a large number of repositories in a GitHub organisation.",
  "author": "Braimbridge, Adam <adam@braimbridge.com> (https://github.com/adambraimbridge)",
  "license": "MIT",
  "repository": "https://github.com/financial-times-sandbox/tako.git",
  "homepage": "https://github.com/financial-times-sandbox/tako",
  "bugs": "https://github.com/financial-times-sandbox/tako/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "lint": "tslint --project test --fix",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test": "jest && tslint --project test",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.4",
    "@types/node": "^10.11.5",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "precise-commits": "^1.0.2",
    "prettier": "1.14.3",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  },
  "engines": {
    "node": ">= 8.12.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  }
}
